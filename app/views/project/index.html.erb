<div class="row" data-no-turbolink>
	<br/>

	<% @projects.each do |project| %>
	<%= link_to project do %>
		<div class="col s12 m6 l4">
			<div class="card">
				<div class="card-image " style="height: 18em">
					<%= image_tag project.image %>
				</div><br/>

				<hr>
				<!-- <div class="center">
					<button class="btn btn-pink">Add to cart</button>
				</div> -->
				<div class="card-content" style="height: 6em">
					<span class="card-title grey-text text-darken-1">
						<h5><%= project.shortname %></h5>
					</span>
					
				</div>
			</div>
		</div>
	<% end %>
	<% end %>
</div>




